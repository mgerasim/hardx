<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                    assembly="HardX"
                   namespace="HardX.Models">

  <class name="Shipping" table="SHIPPINGS">

    <id name="ID" column="id" type="int">

      <generator class="sequence">
        <param name="sequence">SEQ_SHIPPINGS</param>
      </generator>
    </id>
    <property name="Name"  column="name" type="String" length="255"/>
    <property name="Count" column="Count" type="int" />
    <many-to-one name="Devmodel" column="DEVICE_ID" />
    <many-to-one name="Matmodel" column="MATERIAL_ID" />
  </class>
</hibernate-mapping>